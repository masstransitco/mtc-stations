{
  "crons": [
    {
      "path": "/api/cron/carpark-vacancy",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/parking-space-occupancy",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/metered-carpark-occupancy",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/trending-refresh",
      "schedule": "1-59/5 * * * *"
    },
    {
      "path": "/api/cron/cleanup-metered-snapshots",
      "schedule": "0 3 * * 0"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
